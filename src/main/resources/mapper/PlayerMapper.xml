<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.msc.mapper.PlayerMapper">
    <select id="findById" resultType="com.msc.model.entity.Player">
        select * from players where id = #{id}
